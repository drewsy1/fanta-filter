{
    "compilerOptions": {
        "outDir": "./dist",
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es5",
        "allowJs": true,
        "sourceMap": true,
        "lib": ["dom", "es6", "es5"],
        "baseUrl": "./",
        "strictFunctionTypes": false,
        "paths": {
            "Interfaces": ["src/ts/lib/interfaces/index.ts"],
            "Util": ["src/ts/lib/util/index.ts"]
        }
    },
    "include": ["src/**/*.ts", "test/**/*.ts"],
    "exclude": ["node_modules"]
}
